  <POU Name="FB_Eventlogger" Id="{54fe8374-df8d-45a4-bac0-640d990f252a}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_Eventlogger
VAR_INPUT
	eventClass: GUID;
	nEventID  : UDINT;
//	sString   : STRING;
END_VAR
VAR_OUTPUT
END_VAR
VAR
	fbEventLogger : FB_TcEventLogger;
	fbSourceInfo  : FB_TcSourceInfo;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[fbSourceInfo.Clear();
	fbSourceInfo.sName := '';
	fbSourceInfo.nId := 1;	
